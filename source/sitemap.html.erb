<div role="main" class="main">
	<section class="page-header">
		<div class="container">
			<div class="row">
				<div class="col-md-12">
					<ul class="breadcrumb">
						<li><a href="#">Home</a></li>
						<li class="active">Pages</li>
					</ul>
				</div>
			</div>
			<div class="row">
				<div class="col-md-12">
					<h1>Sitemap</h1>
				</div>
			</div>
		</div>
	</section>

	<div class="container">

		<div class="row">

			<div class="col-md-4">
				<ul class="nav nav-list mb-xl">
					<% pages = sitemap.resources.find_all{|p| p.source_file.match(/\.html/) && !p.data.noindex == true && p.data.title } %>
					<% pages.each do |page| %>
						<li><a href="<%= page.destination_path %>"><%= page.data.title %></a></li>
					<% end %>
					
					<% #<li>
						#<a href="#">About Us</a>
